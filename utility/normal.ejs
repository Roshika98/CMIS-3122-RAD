<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    </head>

    <body>
        <div style="text-align: center;">
            <span style="font-size:13px;">
                <em>faculty of Applied Sciences</em>
                <br>
                <em>Wayamba Univesity of Sri Lanka</em>
            </span>
        </div>
        <br>
        <h6 style="text-align: center;">
            <span>
                COURSE REGISTRATION FORM
                <br>
                Academic year: <%= data.personal.academicYear %> &lbbrk;level <%= data.personal.level %>&rbbrk;
            </span>
        </h6>
        <div>
            <%= data.personal.name %><br>
            <%= data.personal.regNo %><br>
            <%= data.personal.contact %><br>
            <%= data.personal.level %>
            <%= data.personal.academicYear %>
        </div>
        <br>
        <h6>Semester I</h6>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">Module Code</th>
                    <th scope="col">Title of the Compulsory Module</th>
                    <th scope="col">No.of Credits</th>
                </tr>
            <tbody>
                <% var sem1Mandatory=data.semester1.mandatory %>
                <% for( let i = 0; i < sem1Mandatory.length; i++ ) { %>
                <tr>
                    <td style="vertical-align:middle"><%= sem1Mandatory[i].code %></td>
                    <td style="vertical-align:middle"><%= sem1Mandatory[i].name %></td>
                    <td style="vertical-align:middle"><%= sem1Mandatory[i].credit %></td>
                </tr>
                <% } %>
            </tbody>
            </thead>
        </table>


        <div class="html2pdf__page-break"></div>
        <br>
        <h6>Semester II</h6>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">Module Code</th>
                    <th scope="col">Title of the Compulsory Module</th>
                    <th scope="col">No.of Credits</th>
                </tr>
            <tbody>
                <% var sem2Mandatory=data.semester2.mandatory %>
                <% for( let i = 0; i < sem2Mandatory.length; i++ ) { %>
                <tr>
                    <td style="vertical-align:middle"><%= sem2Mandatory[i].code %></td>
                    <td style="vertical-align:middle"><%= sem2Mandatory[i].name %></td>
                    <td style="vertical-align:middle"><%= sem2Mandatory[i].credit %></td>
                </tr>
                <% } %>
            </tbody>
            </thead>
        </table>



        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js"
            integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB"
            crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js"
            integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13"
            crossorigin="anonymous"></script>

    </body>

</html>