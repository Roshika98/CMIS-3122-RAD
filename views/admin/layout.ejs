<!DOCTYPE html>
<html lang="en">

    <head>
        <title><%= layoutVar.title %></title>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />

        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

        <!--     Fonts and icons     -->
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />

        <!-- Material Kit CSS -->
        <link href="/assets/css/bootstrap.min.css" rel="stylesheet" />
        <link href="/assets/css/now-ui-dashboard.css" rel="stylesheet" />
        <link rel="stylesheet" href="/css/personal.css" />
        <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
        <script src="/assets/js/core/jquery.min.js" type="text/javascript"></script>
        <script src="/assets/js/plugins/bootstrap-notify.js"></script>
    </head>

    <body>
        <%- include('./notifications/success')  %>
        <%- include('./notifications/error')  %>
        <%- include('./notifications/warning')  %>
        <div class="wrapper">
            <div class="sidebar" data-color="yellow">
                <!--
      Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
  -->
                <div class="logo">
                    <img src="https://fas.wyb.ac.lk/wp-content/uploads/2020/10/center_white.png" alt="" width="237"
                        height="61" class="d-inline-block align-text-top">
                </div>

                <div class="sidebar-wrapper" id="sidebar-wrapper">
                    <ul class="nav">
                        <li id="homepage">
                            <a href="https://localhost:3000/courses/admin/homepage">

                                <i class="fa fa-tachometer" aria-hidden="true"></i>

                                <p>Dashboard</p>
                            </a>
                        </li>

                        <li id="departmentspage">
                            <a href="https://localhost:3000/courses/admin/departments">

                                <i class="fa fa-university" aria-hidden="true"></i>

                                <p>Departments</p>
                            </a>
                        </li>

                        <li id="modulespage">
                            <a href="https://localhost:3000/courses/admin/modules">

                                <i class="fa fa-graduation-cap" aria-hidden="true"></i>

                                <p>Modules</p>
                            </a>
                        </li>

                        <li id="noticepage">
                            <a href="https://localhost:3000/courses/admin/notices">

                                <i class="fa fa-newspaper-o" aria-hidden="true"></i>

                                <p>Notices</p>
                            </a>
                        </li>

                        <li id="accountspage">
                            <a href="https://localhost:3000/courses/admin/account">

                                <i class="now-ui-icons users_single-02"></i>

                                <p>User Profile</p>
                            </a>
                        </li>

                        <li id="statpage">
                            <a href="https://localhost:3000/courses/admin/statistics">

                                <i class="fa fa-bar-chart" aria-hidden="true"></i>

                                <p>Statistics</p>
                            </a>
                        </li>

                        <li style="position: absolute;width: 100%;bottom: 10px;">
                            <a href="https://localhost:3000/courses/admin/logout">
                                <i class="fa fa-sign-out" aria-hidden="true"></i>
                                <p>Sign out</p>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="main-panel" id="main-panel">
                <!-- Navbar -->
                <nav class="navbar navbar-expand-lg   navbar-transparent  bg-primary  navbar-absolute">
                    <div class="container-fluid">
                        <div class="navbar-wrapper">
                            <div class="navbar-toggle">
                                <button type="button" class="navbar-toggler ">
                                    <span class="navbar-toggler-bar bar1 bg-dark"></span>
                                    <span class="navbar-toggler-bar bar2 bg-dark"></span>
                                    <span class="navbar-toggler-bar bar3 bg-dark"></span>
                                </button>
                            </div>
                            <a class="navbar-brand " href="#pablo" style="color: black;font-size: large;">
                                DASHBOARD
                            </a>
                        </div>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation"
                            aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-bar navbar-kebab bg-dark"></span>
                            <span class="navbar-toggler-bar navbar-kebab bg-dark"></span>
                            <span class="navbar-toggler-bar navbar-kebab bg-dark"></span>
                        </button>
                        <div class="collapse navbar-collapse justify-content-end" id="navigation">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" href="#pablo">
                                        <i class="now-ui-icons media-2_sound-wave" style="color: black;"></i>
                                        <p>
                                            <span class="d-lg-none d-md-block">Stats</span>
                                        </p>
                                    </a>
                                </li>
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink"
                                        data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <i class="now-ui-icons location_world" style="color: black;"></i>
                                        <p>
                                            <span class="d-lg-none d-md-block">Some Actions</span>
                                        </p>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-right"
                                        aria-labelledby="navbarDropdownMenuLink">
                                        <a class="dropdown-item" href="#">Action</a>
                                        <a class="dropdown-item" href="#">Another action</a>
                                        <a class="dropdown-item" href="#">Something else here</a>
                                    </div>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="https://localhost:3000/courses/admin/account">
                                        <i class="now-ui-icons users_circle-08" style="color: black;"></i>
                                        <p>
                                            <span class="d-lg-none d-md-block">Account</span>
                                        </p>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
                <!-- End Navbar -->
                <div class="panel-header panel-header-md"
                    style="background-image: url('/img/FB_IMG_1653584547326.jpg');background-position: center; background-size:cover; background-repeat: no-repeat;">
                </div>
                <div class="content">
                    <%- body %>
                </div>
                <footer class="footer">
                    <div class=" container-fluid ">
                        <div class="copyright" id="copyright">
                            &copy;
                            <script>
                                document.getElementById('copyright').appendChild(document.createTextNode(new Date().getFullYear()))
                            </script>
                            Wayamba University of Sri Lanka
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </body>
    <!--   Core JS Files   -->

    <script src="/assets/js/core/popper.min.js" type="text/javascript"></script>
    <script src="/assets/js/core/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/js/plugins/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <!-- Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="/assets/js/now-ui-dashboard.js" type="text/javascript"></script>
    <script src="<%= layoutVar.script %>"></script>

</html>