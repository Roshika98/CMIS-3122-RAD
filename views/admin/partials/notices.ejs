<div class="row">
    <div class="col-md-12">
        <div class="card card-nav-tabs" id="mainDisplay">
            <div class="card-header card-header-danger">
                <!-- colors: "header-primary", "header-info", "header-success", "header-warning", "header-danger" -->
                <div class="nav-tabs-navigation">
                    <div class="nav-tabs-wrapper">
                        <ul class="nav nav-tabs" data-tabs="tabs">
                            <li class="nav-item">
                                <a class="nav-link active" href="#home" data-toggle="tab">Notices</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#newModule" data-toggle="tab">Add Notice</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="card-body ">
                <div class="tab-content">
                    <div class="tab-pane active" id="home">
                        <h4 class="card-title"> Notices</h4>

                        <div class="table-responsive">
                            <table class="table">
                                <thead class=" text-primary">
                                    <th>
                                        Heading
                                    </th>
                                    <th>
                                        Date
                                    </th>
                                    <th class="text-right">

                                    </th>
                                </thead>
                                <tbody>

                                    <% result.forEach(element => { %>
                                    <tr>
                                        <td>
                                            <%= element.heading %>
                                        </td>
                                        <td>
                                            <%= element.noticeDate.toDateString() %>
                                        </td>
                                        <td class="text-right">
                                            <div class="container">
                                                <form class="form-inline ml-auto">
                                                    <!-- <div class="col-md-6 pr-1">
                                                        <button type="submit"
                                                            class="btn btn-outline-default btn-round btn-sm editBtns"
                                                            data-id="">
                                                            edit
                                                        </button>
                                                    </div> -->
                                                    <div class="col-md-2 pl-1">
                                                        <button type="submit"
                                                            class="btn btn-danger btn-round btn-sm deleteBtns"
                                                            data-id="<%= element.filename %>">
                                                            delete
                                                        </button>
                                                    </div>
                                                </form>
                                            </div>
                                        </td>
                                    </tr>
                                    <% }) %>



                                </tbody>
                            </table>
                        </div>

                    </div>
                    <div class="tab-pane" id="newModule">
                        <h5 class="title"><span><i class="fa fa-plus-circle"></i></span> Add New Notice</h5>

                        <form action="https://localhost:3000/courses/admin/notices" method="post"
                            enctype="multipart/form-data">
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label for="heading">Heading</label>
                                        <input type="text" class="form-control" placeholder="heading" name="heading"
                                            id="heading">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4 pr-1">
                                    <div class="input-group">
                                        <input type="file" class="form-control " id="fileInput" name="fileInput">
                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-md-4 pr-1">
                                    <div class="form-group">
                                        <button class="btn btn-primary btn-round" id="submitbtn">
                                            <i class="fa fa-refresh" aria-hidden="true"></i> Upload notice
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
        <div class="card" id="editContentDisplay">

        </div>
    </div>
</div>